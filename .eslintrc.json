{
    "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended", "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "VERSION": true,
        "COMMITHASH": true,
        "BRANCH": true,
        "showAjaxWaitDialog": true,
        "closeAjaxWaitDialog": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
    ],
    "rules": {
        "indent": [
            "error",
            4,
            {"SwitchCase": 1}
        ],
        "linebreak-style": [
            "off",
            "unix"
        ],
        "semi": [
            "warn",
            "always"
        ],
        
        "comma-dangle": 0,
        "react/jsx-uses-vars": 1,
        "react/display-name": 1,
        "no-unused-vars": ["warn", {"vars": "local"}],
        "no-console": "off",
        "no-unexpected-multiline": "warn",
        "no-extra-boolean-cast": "off",
        "react/prop-types": [2, { "skipUndeclared": true }]
    },
    "settings": {
        "react": {
            "pragma": "React",
            "version": "detect"
        }
    }
}